#include "Xmastm.tmpl"

#ifndef ForceSubdirs
#define ForceSubdirs(dirs)
#endif
#define IHaveSubdirs
#define PassCDebugFlags CCOPTIONS='$(CCOPTIONS)'

DEPLIBS= ElecDisp all

SRCS=	main.c xerror.c compat.c usage.c error.c crmainw.c alncolor.c\
        bestpix.c setcol.c commitcb.c mkcode.c scorem.c fmenucb.c\
        setscore.c restart.c xmspec.c mpulldown.c helpcb.c\
        craskd.c askcb.c craskd2.c

OBJS=	main.o xerror.o compat.o usage.o error.o crmainw.o alncolor.o\
        bestpix.o setcol.o commitcb.o mkcode.o scorem.o fmenucb.o\
        setscore.o restart.o xmspec.o mpulldown.o helpcb.o\
        craskd.o askcb.o craskd2.o

